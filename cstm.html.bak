<script>
		$(document).ready(function () {
            $("#show_less").hide();
			$("#show_more").click(function (e) {
                e.preventDefault();

                $("#outcomes_extra").removeClass("d-none");
                $("#outcomes_extra").hide();
                $("#outcomes_extra").slideDown(100);
                $("#show_less").removeClass("d-none");
                $("#show_less").show();
                $(this).hide();

			});

			$("#show_less").click(function (e) {
                e.preventDefault();

                $("#outcomes_extra").slideUp(100);
                $(this).hide();
                $("#show_less").addClass("d-none");
                $("#show_more").show();

			});
		});

	</script>
 <script>
        function more(i){
                // alert(1)
                $("#outcomes_extra_"+i).removeClass("d-none");
                $("#outcomes_extra_"+i).hide();
                $("#outcomes_extra_"+i).slideDown(100);
                $("#show_more_network_"+i).hide();
                $("#show_less_network_"+i).show();
                $(this).hide();
            }

            function less(i){
                // alert(2)
                $("#outcomes_extra_"+i).slideUp(100);
                $(this).hide();
                $("#show_less_network_"+i).hide();
                $("#show_more_network_"+i).show();
            }
